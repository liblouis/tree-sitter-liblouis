{
  "name": "tree-sitter-liblouis",
  "version": "1.0.0",
  "description": "Tree sitter for liblouis braille tables",
  "main": "bindings/node",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "liblouis",
    "braille"
  ],
  "author": "Christian Egli",
  "license": "ISC",
  "dependencies": {
    "nan": "^2.15.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.1"
  },
  "tree-sitter": [
    {
      "scope": "source.liblouis",
      "file-types": [
        "cti",
        "uti",
        "ctb",
        "utb",
        "tbl",
	"dis"
      ],
      "first-line-regex": "# liblouis: "
    }
  ]
}
