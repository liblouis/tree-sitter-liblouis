{
  "name": "tree-sitter-liblouis",
  "version": "1.0.1",
  "description": "Tree sitter for liblouis braille tables",
  "main": "bindings/node",
  "scripts": {
    "test": "tree-sitter test"
  },
  "keywords": [
    "liblouis",
    "braille",
    "parser"
  ],
  "homepage": "https://github.com/liblouis/tree-sitter-liblouis#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/liblouis/tree-sitter-liblouis.git"
  },
  "author": "Christian Egli",
  "license": "ISC",
  "dependencies": {
    "nan": "^2.15.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.1"
  },
  "tree-sitter": [
    {
      "scope": "source.liblouis",
      "file-types": [
        "cti",
        "uti",
        "ctb",
        "utb",
        "tbl",
        "dis"
      ],
      "first-line-regex": "# liblouis: "
    }
  ]
}
