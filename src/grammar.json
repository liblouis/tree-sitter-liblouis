{
  "name": "liblouis",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_line1"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_line1": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "_rule_with_comment"
        },
        {
          "type": "SYMBOL",
          "name": "_empty_line"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "SYMBOL",
          "name": "_not_newline"
        }
      ]
    },
    "end_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_sp"
        },
        {
          "type": "SYMBOL",
          "name": "_not_newline"
        }
      ]
    },
    "_not_newline": {
      "type": "PATTERN",
      "value": "[^\\n]*"
    },
    "_empty_line": {
      "type": "SYMBOL",
      "name": "_sp_maybe"
    },
    "_rule_with_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_sp_maybe"
        },
        {
          "type": "SYMBOL",
          "name": "_rule"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "end_comment"
            },
            {
              "type": "SYMBOL",
              "name": "_sp_maybe"
            }
          ]
        }
      ]
    },
    "_rule": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "include"
        },
        {
          "type": "SYMBOL",
          "name": "undefined"
        },
        {
          "type": "SYMBOL",
          "name": "display"
        },
        {
          "type": "SYMBOL",
          "name": "letter"
        }
      ]
    },
    "include": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "include"
        },
        {
          "type": "SYMBOL",
          "name": "_sp"
        },
        {
          "type": "SYMBOL",
          "name": "filename"
        }
      ]
    },
    "undefined": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "undefined"
        },
        {
          "type": "SYMBOL",
          "name": "_sp"
        },
        {
          "type": "SYMBOL",
          "name": "dots_with_zero"
        }
      ]
    },
    "display": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "display"
        },
        {
          "type": "SYMBOL",
          "name": "_sp"
        },
        {
          "type": "SYMBOL",
          "name": "chars"
        },
        {
          "type": "SYMBOL",
          "name": "_sp"
        },
        {
          "type": "SYMBOL",
          "name": "dots_with_zero"
        }
      ]
    },
    "letter": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "letter"
        },
        {
          "type": "SYMBOL",
          "name": "_sp"
        },
        {
          "type": "SYMBOL",
          "name": "chars"
        },
        {
          "type": "SYMBOL",
          "name": "_sp"
        },
        {
          "type": "SYMBOL",
          "name": "_dots_with_zero_or_equal"
        }
      ]
    },
    "_dots_with_zero_or_equal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "equal"
        },
        {
          "type": "SYMBOL",
          "name": "dots_with_zero"
        }
      ]
    },
    "equal": {
      "type": "STRING",
      "value": "="
    },
    "dots_with_zero": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "dot_with_zero"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "-"
              },
              {
                "type": "SYMBOL",
                "name": "dot_with_zero"
              }
            ]
          }
        }
      ]
    },
    "dot_with_zero": {
      "type": "PATTERN",
      "value": "[0-9a-f]+"
    },
    "esc_seq": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "\\\\"
        },
        {
          "type": "STRING",
          "value": "\\f"
        },
        {
          "type": "STRING",
          "value": "\\n"
        },
        {
          "type": "STRING",
          "value": "\\r"
        },
        {
          "type": "STRING",
          "value": "\\s"
        },
        {
          "type": "STRING",
          "value": "\\t"
        },
        {
          "type": "STRING",
          "value": "\\v"
        },
        {
          "type": "STRING",
          "value": "\\e"
        }
      ]
    },
    "chars": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "esc_seq"
        },
        {
          "type": "PATTERN",
          "value": "[^ \\t\\n]+"
        }
      ]
    },
    "filename": {
      "type": "PATTERN",
      "value": "[-a-zA-Z0-9._\\/]+"
    },
    "_newline": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "STRING",
          "value": "\r\n"
        },
        {
          "type": "STRING",
          "value": "\r"
        }
      ]
    },
    "_sp": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    "_sp_maybe": {
      "type": "PATTERN",
      "value": "[ \\t]*"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

